pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building....'
                bat 'mvn clean'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
                 bat 'mvn clean compile'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
                bat 'mvn test'
            }
        }
    }
}